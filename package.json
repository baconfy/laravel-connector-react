{
  "name": "laravel-connector-react",
  "version": "1.0.3",
  "description": "React hooks for laravel-connector - Seamless integration between React and Laravel APIs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "laravel",
    "react",
    "hooks",
    "api",
    "sanctum",
    "typescript",
    "nextjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/baconfy/laravel-connector-react.git"
  },
  "author": "Baconfy",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^19.2",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^20.0.7",
    "react": "^19.2",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "laravel-connector": "^1.3",
    "@testing-library/react": "^16.3.0"
  }
}
